{
  "name": "electron-nyamadandan",
  "version": "1.0.0",
  "description": "Electron Nyamadandan",
  "main": "./index.js",
  "author": "Yoshiki YAMADA",
  "license": "UNLICENSED",
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.16",
    "@storybook/addon-info": "^4.0.0-alpha.16",
    "@storybook/addon-links": "^4.0.0-alpha.16",
    "@storybook/react": "^4.0.0-alpha.16",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.6",
    "@types/react-virtualized": "^9.18.5",
    "@types/storybook__addon-actions": "^3.4.0",
    "@types/storybook__addon-info": "^3.4.1",
    "@types/storybook__addon-links": "^3.3.1",
    "@types/storybook__react": "^3.0.8",
    "@types/uuid": "^3.4.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.5",
    "electron-packager": "^12.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "react": "^16.4.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.22.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "scripts": {
    "prestart:development": "npm run build:main:development",
    "start:development": "cd ./dist && cross-env NODE_ENV=development electron ./index.js",
    "build:development": "npm run build:main:development && npm run build:renderer:development",
    "prebuild:main:development": "cpx \"{devtools.json,yarn.lock,package.json}\" \"./dist\" -C",
    "build:main:development": "cross-env NODE_ENV=development webpack --config webpack.main.config.js",
    "build:renderer:development": "cross-env NODE_ENV=development webpack --config webpack.renderer.config.js",
    "watch:renderer:development": "cross-env NODE_ENV=development webpack --watch --config webpack.renderer.config.js",
    "serve:renderer:development": "cross-env NODE_ENV=development webpack-dev-server --config webpack.renderer.config.js",
    "prestart:production": "npm run build:main:production",
    "start:production": "cd ./dist && cross-env NODE_ENV=production electron ./index.js",
    "build:production": "npm run build:main:production && npm run build:renderer:production",
    "prebuild:main:production": "cpx \"{yarn.lock,package.json}\" \"./dist\" -C",
    "build:main:production": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "build:renderer:production": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js",
    "bundle:win32": "electron-packager ./dist electron-nyamadandan --platform=win32 --arch=x64 --overwrite",
    "bundle:darwin": "electron-packager ./dist electron-nyamadandan --platform=darwin --arch=x64 --overwrite",
    "clean": "rimraf \"./electron-nyamadandan-*\" \"./dist\" \"*.log\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {},
  "private": true
}
